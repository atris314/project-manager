        "/api/v1/register": {
            "post": {
                "tags": [
                    "Register"
                ],
                "summary": "ثبت نام کاربر",
                "description": "ثبت نام",
                "operationId": "register",
                "requestBody": {
                    "description": "",
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "mobile",
                                    "password",
                                    "email",
                                    "name"
                                ],
                                "properties": {
                                    "mobile": {
                                        "type": "number",
                                        "format": "string",
                                        "example": "شماره موبایل مثال : 09123456789"
                                    },
                                    "password": {
                                        "type": "text",
                                        "format": "string",
                                        "example": " رمز عبور خود را وارد کنید "
                                    },
                                    "email": {
                                        "type": "text",
                                        "format": "string",
                                        "example": " info@example.com "
                                    },
                                    "name": {
                                        "type": "text",
                                        "format": "string",
                                        "example": " نام خود را وارد کنید "
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {
                                "required": [
                                    "mobile",
                                    "password",
                                    "email",
                                    "name"
                                ],
                                "properties": {
                                    "mobile": {
                                        "type": "number",
                                        "format": "string",
                                        "example": "شماره موبایل مثال : 09123456789"
                                    },
                                    "password": {
                                        "type": "text",
                                        "format": "string",
                                        "example": " رمز عبور خود را وارد کنید "
                                    },
                                    "email": {
                                        "type": "text",
                                        "format": "string",
                                        "example": " info@example.com "
                                    },
                                    "name": {
                                        "type": "text",
                                        "format": "string",
                                        "example": " نام خود را وارد کنید "
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "ثبت نام انجام شد",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string",
                                            "example": "eyJ0eXAiOiJKV......."
                                        },
                                        "token_type": {
                                            "type": "string",
                                            "example": "bearer"
                                        },
                                        "user_mobile": {
                                            "type": "string",
                                            "example": "09123..."
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "ثبت نام با موفقیت انجام شد"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "خطایی رخ داده است",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "خطایی رخ داده است"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
